/data/*.sqlite-journal
/data/*.sqlite-wal
/data/*.sqlite-shm

# Commit the main database.sqlite file for production (serverless deployment)
# Uncomment the line below if you want to ignore the database in development
# /data/database.sqlite

!/data/.gitkeep
